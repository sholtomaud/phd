@article{Campbella,
  title = {Emergy analysis of emerging methods of fossil fuel production},
  issn = {0304-3800},
  url = {http://www.sciencedirect.com/science/article/pii/S0304380014005547},
  doi = {10.1016/j.ecolmodel.2014.10.036},
  abstract = {The production of energy from unconventional sources of fossil fuels, (e.g., tar sands, shale or sandstone formations where oil or natural gas is tightly bound), is forming an increasingly large proportion of global energy use. This research evaluates three of the most prominent emerging methods of fossil fuel production{\textemdash}oil from oil/tar sands, shale oil and shale gas, both produced through hydraulic fracturing. In 2013, 42\% of natural gas production and 41\% of oil production in the United States was produced through hydraulic fracturing and \ensuremath{\sim}60\% of new oil and gas wells use the method. In 2013, the Alberta region of Canada produced 1.7 million barrels per day of synthetic crude oil from oil sands, exporting 1.3 million barrels to the US every day, comprising \ensuremath{\sim}7\% of daily US oil consumption. Results from this work show that natural gas obtained through hydraulic fracturing has an emergy yield ratio (EYR) between 4.23 and 9.18, depending on well productivity and whether the gas is ``wet'' (contains other hydrocarbons) or ``dry'' (pure or nearly pure methane). Synthetic crude oil from oil sands was found to have an EYR of 3.38{\textendash}4.06 (in situ vs. mined production, respectively). The EYR of tight oil was found to be 3.43{\textendash}4.73, dependent on the productivity of the well. Given the relatively high current market price of oil and relatively low market price of natural gas the observed results (i.e., similar or higher EYR for natural gas than past observations and a lower EYR for oil than past observations) are expected.},
  timestamp = {2015-01-23 03:07:18},
  urldate = {2015-01-23},
  journal = {Ecological Modelling},
  author = {Campbell, Elliott T.},
  keywords = {Emergy yield,Environmental accounting,Hydraulic fracturing,Net energy,Oil sands}
}

