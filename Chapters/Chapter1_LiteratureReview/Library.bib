@article{Shi2006,
  title = {Bilirubin potentiates inhibitory synaptic transmission in lateral superior olive neurons of the rat},
  volume = {55},
  lccn = {0005},
  url = {http://www.sciencedirect.com/science/article/B6T0H-4JMM5J4-2/2/f289f9e642a7e66f75ac09139f346291},
  doi = {10.1016/j.neures.2006.02.015},
  abstract = {Bilirubin is a well-known neurotoxin that can result in multiple neurologic deficits. Previous studies have suggested that bilirubin affects aspects of synaptic transmission; however the acute effects of bilirubin on synaptic transmission have not been examined in real-time. In this study, using whole-cell voltage-clamp recordings, we observed the effect of bilirubin on inhibitory postsynaptic currents (IPSC) in postnatal 13-15-day-old neurons dissociated from lateral superior olive nuclei (LSO), one of the brainstem auditory nucleus that are highly vulnerable to bilirubin. The results showed that 10-5�M bilirubin increased the frequency of spontaneous IPSC without causing change in their amplitudes or in the response to bath applied glycine, suggesting a presynaptic locus for the action. In the presence of tetrodotoxin, the frequency of miniature IPSC was also potentiated by 10-5�M bilirubin. The facilitation by bilirubin was concentration dependent and increased with an increase in exposure time. Bicuculline only partially reduced the action of bilirubin. The action of bilirubin was observed in extracellular Ca2+-free ({[}Ca2+{]}o free) solution but was fully occluded by pretreatment with BAPTA-AM in {[}Ca2+{]}o free solution. Thus, in LSO neurons, bilirubin facilitates inhibitory synaptic transmission, in a manner independent of voltage-activated Na+ and Ca2+ channels but dependent on presynaptic {[}Ca2+{]}i. The increase of inhibitory synaptic transmission in response to acute bilirubin is a novel effect of bilirubin on the central nervous system and may have implications for neurotoxicity and the impairment of auditory transduction seen in hyperbilirubinemia.},
  timestamp = {2010-04-04 00:27:15},
  number = {2},
  urldate = {2008-08-17},
  journal = {Neuroscience Research},
  author = {Shi, Hai-Bo and Kakazu, Yasuhiro and Shibata, Shumei and Matsumoto, Nozomu and Nakagawa, Takashi and Komune, Shizuo},
  month = jun,
  year = {2006},
  keywords = {Bilirubin,IPSCs,Lateral superior olive neurons,Neurotoxicity,Patch clamp,Presynaptic facilitation},
  pages = {161--170}
}

